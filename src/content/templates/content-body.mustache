{{#title}}
    {{#feedUrl}}
        <a href="{{ feedUrl }}" target="_blank" class="content-body-title">
            <strong>{{{ title }}}</strong>
        </a>
    {{/feedUrl}}
    {{^feedUrl}}
        <p class="content-body-title"><strong>{{{ title }}}</strong></p>
    {{/feedUrl}}
{{/title}}

<div class="content-body-main" tabindex="0" aria-label="{{ text }}">{{{ body }}}</div>

{{#featured}}
    <div class="content-featured">{{{ featuredText }}}</div>
{{/featured}}
